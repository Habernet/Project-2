<div class="container">
  {{!-- <div class="text-center">Devour!</div> --}}
  <div class="homeBtn">
    <a href="/dinein" class="btn routeBtn " role="button">Dine-in?</span></a>
    <a href="/"><button class="btn btn-lg btn-default"><span class="fa fa-home"></span></button></a>


  </div>
</div>
<div id="menuDiv" class="container">
  <div id="menu">

    {{#each data}}
    <div class="card" style="width: 18rem;">
      <div class="card-body">
        <img class="card-img-top" src={{url}} alt="Card image cap" ; style="width:100%;height:200px">

        <details class="menuItem">
          <summary class="name">{{name}}</summary>
          {{this.description}}
        </details>
        <p class="card-text price">{{price}}</p>
        <div class="btnDiv">
          <select class="quantity">
            <option value="1">1</option>
            <option value="2">2 </option>
            <option value="3">3</option>
            <option value="4">4</option>
            <option value="5">5</option>
            <option value="6">6</option>
            <option value="7">7</option>
            <option value="8">8</option>
            <option value="9">9</option>
            <option value="10">10</option>
          </select>
          <button class="addtoCartBtn btn" id="placeorderBtn"><span class="fa fa-shopping-bag"></span> </button>
        </div>
      </div>
    </div>

    {{/each}}
  </div>
</div>

<div class="container">
  <div class="row">
  </div>
  <div class="col-3" id="cart">
    <img id="bagImg" src="styles/images.png"></img>
    <p class="text-muted">Your bag is empty!! Add your favorites</p>
  </div>
</div>
<div class="container" id="cartwithOrder">
  <table>
    <tbody>

    </tbody>
  </table>


  <div class="payment">
    <form class="row " method="POST" action="/echo">
      <div class="form-group owner">
        <h5 class="blink_me">Enter your details here to place order!</h5>
        <label for="owner">Card Owner</label>
        <input type="text" class="form-control" id="owner">
        <span class="text-muted">Full name as displayed on the card</span>

      </div>
      <div class="form-group phone">
        <label for="phone">Phone number</label>
        <input type="text" class="form-control" id="phoneNum">
      </div>

      <div class="form-group CVV">
        <label for="cvv">CVV</label>
        <input type="text" class="form-control" id="cvv">
        <span class="text-muted">CVV/CVC</span>


      </div>
      <div class="form-group" id="card-number-field">
        <label for="cardNumber">Card Number</label>
        <input type="text" class="form-control" name="cc-number" id="cardNumber">
        <span class="text-muted">Enter credit card number here</span>


        {{!-- <input type="text" class="form-control" id="cardNumber"> --}}
      </div>
      <div class="form-group" id="expiration-date">
        <label>Expiration Date</label>
        <select>
          <option value="01">January</option>
          <option value="02">February </option>
          <option value="03">March</option>
          <option value="04">April</option>
          <option value="05">May</option>
          <option value="06">June</option>
          <option value="07">July</option>
          <option value="08">August</option>
          <option value="09">September</option>
          <option value="10">October</option>
          <option value="11">November</option>
          <option value="12">December</option>
        </select>
        <select>
          <option value="20"> 2020</option>
          <option value="21"> 2021</option>
          <option value="22"> 2022</option>
          <option value="23"> 2023</option>
          <option value="24"> 2024</option>
          <option value="25"> 2025</option>

        </select>
      </div>
      <div class="form-group">
        <button class="btn btn-danger" id="confirmPurchase">Confirm <span class="fa fa-check-circle"></button>
      </div>
    </form>
  </div>
</div>




<div class="modal fade" id="myModal" role="dialog" style="opacity:1">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Order Summary!</h4>

      </div>
      <div class="modal-body " style="text-align:center;font-size: 1.5em;">
      </div>
      <div class="modal-bodyimg " style="text-align:center;font-size: 2em;">
      </div>

      <div class="modal-footer">

        <a href="#" id="editBtn" class="btn btn-primary" data-dismiss="modal" data-toggle="tooltip" data-trigger="hover"
          title="Edit order"><span class="fa fa-edit"></span>
        </a>

        <a href="#" id="closeBtn" class="btn btn-success" data-dismiss="modal" data-toggle="tooltip"
          data-trigger="hover" title="Place Order!"> <span class="fa fa-check"></span></a>
      </div>
    </div>

  </div>
</div>

<div class="modal fade" id="confirmModal" role="dialog" style="opacity:1">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title"></h4>

      </div>
      <div class="modal-body text-center" style="font-size: 2em;text-align:center;">
      </div>
      <div class="modal-bodyimg " style="text-align:center;font-size: 2em;">
      </div>

      <div class="modal-footer">
        <button id="endokBtn" class="btn btn-success" title="OK!" data-dismiss="modal"> <span
            class="fa fa-check"></span></button>

      </div>
    </div>

  </div>
</div>
<footer>
  <div class="outer">
    <div class="flex autoplay">
      {{#each data1}}
      <div class="box">
        <div>

          {{itemreviewed}} <span class="fa fa-heart"></span>
        </div>

        <div>
          {{name}}

        </div>
        "{{review}}"
      </div>
      {{/each}}
    </div>
  </div>
  <div class="aside">
    <a href="/reviews"><button class="btn btn-warning">Write a review <span class="fa fa-comments"></span></button></a>
  </div>

</footer>


<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
<script src="/js/carryout.js"></script>